{
  "name": "SafeTrace",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Chrome Extension that offers website hostname and content anlyzing",
  "icons": {
    "16": "logo16.jpeg",
    "32": "logo32.jpeg",
    "48": "logo48.jpeg",
    "128": "logo128.jpeg"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "http://127.0.0.1/processor"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}